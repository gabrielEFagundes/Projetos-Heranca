package com.weg.appescola.model;

public class Aluno extends Pessoa{
	
	private int serie;
	
	public Aluno(String nome, String email, int serie) {
		super(nome, email);
		this.serie = serie;
	}

	public int getSerie() {
		return serie;
	}

	public void setSerie(int serie) {
		this.serie = serie;
	}
}